#!/usr/bin/env perl
use strict;
use warnings;
use MYDan::Util::OptConf;
use MYDan::Agent::FileCache;

=head1 SYNOPSIS

 $0 $file1 file2 .. filen

=cut

my $option = MYDan::Util::OptConf->load();
my %o = $option->get()->dump();
 
$option->assert() unless @ARGV;
my $filecache = MYDan::Agent::FileCache->new();

map{ $filecache->save( $_ ); }@ARGV;
exit 0;
